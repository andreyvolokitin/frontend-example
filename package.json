  "scripts": {
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js --config-register @babel/register",
    "debug:dev": "npm run debug -- --env.mode development",
    "debug:prod": "npm run debug -- --env.mode production",
    "start": "nodemon --watch webpack.config.js --watch ./build-utils/webpack.development.js --exec \"webpack-dev-server --env.mode development --config-register @babel/register\"",
    "start:sourcemap": "npm run start -- --env.presets sourcemap",
    "start:measure": "npm run start -- --env.measure true",
    "clean": "rimraf \"./dist\"",
    "build": "npm run clean && webpack --env.mode production --config-register @babel/register",
    "build:analyze": "npm run build -- --env.presets analyze",
    "build:compress": "npm run build -- --env.presets compress",
    "build:measure": "npm run build -- --env.measure true",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:js": "eslint . --fix",
    "lint:css": "stylelint \"css/*.css\" --fix",
    "prettydist": "npm run prettydist:html && npm run prettydist:css",
    "prettydist:css": "(stylelint \"./dist/css/**/!(*.min.css)\" -q --fix || echo \"stylelint failed for some file(s).\") && prettier --write \"./dist/css/**/!(*.min.css)\"",
    "prettydist:html": "posthtml \"dist/*.html\" -c posthtml-cli.config.js",
    "prettydist:js": "prettier --write \"./dist/js/**/!(*.min.js)\"",
    "watch": "webpack --env.mode development --progress --watch --config-register @babel/register"
